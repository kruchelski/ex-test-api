{
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^.*/controllers/(.*)$",
		"^.*/handlers/(.*)$",
		"^.*/routes/(.*)$",
		"^.*/utils/(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true,
	"semi": true,
	"editorconfig": false,
	"singleQuote": true,
	"useTabs": true,
	"trailingComma": "es5",
	"tabWidth": 2,
	"jsxSingleQuote": true,
	"printWidth": 100
}
